			}
		}, {
			"name": "text",
			"type": "extractor",
			"id": "selector_text",
			"params": {
				"selector": ".story-body"
			}
		}]
	}, {
		"id": "youtube",
		"priority": 5,
		"type": "unknown",
		"host": "youtube.com",
